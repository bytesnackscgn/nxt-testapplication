{
  "name": "nxt-testapplication",
  "type": "module",
  "version": "1.0.0",
  "description": "nxt testapplication clickup googlechat webhook",
  "main": "./src/index.js",
  "scripts": {
    "build": "NODE_ENV=production",
    "build:staging": "NODE_ENV=staging tsc -p ./tsconfig.json",
    "dev": "NODE_ENV=development esbuild --bundle --platform=node --target=node18.13 src/index.js --outfile=dist/index.js --watch",
    "dev:start": "nodemon --watch dist/index.js dist/index.js",
    "start": "node ./dist/index.js",
    "test": "test",
    "lint": "eslint --ext .js ./src/",
    "lint:fix": "eslint --ext .js ./src/ --fix"
  },
  "author": "Georgij Michel",
  "license": "MIT",
  "dependencies": {
    "@googleapis/chat": "^17.2.1",
    "@koa/cors": "^4.0.0",
    "axios": "^1.4.0",
    "bee-queue": "^1.5.0",
    "dotenv": "^16.3.1",
    "esbuild": "0.19.2",
    "googleapis": "^126.0.0",
    "install": "^0.13.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.0",
    "koa-session": "^6.4.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "npm": "^9.8.1",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-plugin-node": "^11.1.0"
  }
}
